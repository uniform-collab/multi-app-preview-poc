{
  "rewrites": [
    {
      "source": "/app-orange/:match*",
      "destination": "https://multi-app-preview-poc-app-orange.vercel.app/app-orange/:match*"
    },
    {
      "source": "/app-orange",
      "destination": "https://multi-app-preview-poc-app-orange.vercel.app/app-orange"
    },

    {
      "source": "/api/preview",
      "destination": "https://multi-app-preview-poc-app-purple.vercel.app/app-purple/api/preview",
      "has": [
        {
          "type": "query",
          "key": "path",
          "value": "^(/app-orange)"
        }
      ]
    },
    {
      "source": "/app-purple/:match*",
      "destination": "https://multi-app-preview-poc-app-purple.vercel.app/app-purple/:match*"
    },
    {
      "source": "/app-purple",
      "destination": "https://multi-app-preview-poc-app-purple.vercel.app/app-purple"
    },
    {
      "source": "/api/preview",
      "destination": "https://multi-app-preview-poc-app-purple.vercel.app/app-purple/api/preview",
      "has": [
        {
          "type": "query",
          "key": "path",
          "value": "/app-purple.*"
        }
      ]
    }
  ]
}
